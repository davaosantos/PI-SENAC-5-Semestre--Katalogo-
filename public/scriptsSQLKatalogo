'estabelecimentos', 'CREATE TABLE `estabelecimentos` (\n  `id` int NOT NULL AUTO_INCREMENT,\n  `nome` varchar(255) NOT NULL,\n  `descricao` varchar(255) NOT NULL,\n  `regiao_id` int NOT NULL,\n  `name` varchar(255) DEFAULT NULL,\n  `imagem` mediumblob,\n  PRIMARY KEY (`id`),\n  KEY `fk_estabelecimentos_regioes` (`regiao_id`),\n  CONSTRAINT `fk_estabelecimentos_regioes` FOREIGN KEY (`regiao_id`) REFERENCES `regioes` (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=24 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'



'pedido', 'CREATE TABLE `pedido` (\n  `id` int NOT NULL AUTO_INCREMENT,\n  `valor_total` double DEFAULT NULL,\n  `data_pedido` date DEFAULT NULL,\n  `regiao_id` int DEFAULT NULL,\n  `username` varchar(50) DEFAULT NULL,\n  `endereco_entrega` varchar(100) DEFAULT NULL,\n  `nome_comerciante` varchar(255) DEFAULT NULL,\n  `regiao` int DEFAULT NULL,\n  PRIMARY KEY (`id`),\n  KEY `FKdcvs3elksrhqcpycxesc1p4uh` (`regiao_id`),\n  CONSTRAINT `FKdcvs3elksrhqcpycxesc1p4uh` FOREIGN KEY (`regiao_id`) REFERENCES `regioes` (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=33 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'


'pedido_produto', 'CREATE TABLE `pedido_produto` (\n  `pedido_id` int DEFAULT NULL,\n  `produto_id` bigint DEFAULT NULL,\n  `quantidade` int DEFAULT NULL,\n  `id` bigint NOT NULL,\n  KEY `fk_pedido_produto_pedido` (`pedido_id`),\n  KEY `fk_pedido_produto_produto` (`produto_id`),\n  CONSTRAINT `fk_pedido_produto_pedido` FOREIGN KEY (`pedido_id`) REFERENCES `pedido` (`id`),\n  CONSTRAINT `fk_pedido_produto_produto` FOREIGN KEY (`produto_id`) REFERENCES `produto` (`id`),\n  CONSTRAINT `pedido_produto_ibfk_1` FOREIGN KEY (`pedido_id`) REFERENCES `pedido` (`id`),\n  CONSTRAINT `pedido_produto_ibfk_2` FOREIGN KEY (`produto_id`) REFERENCES `produto` (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'



'produto', 'CREATE TABLE `produto` (\n  `id` bigint NOT NULL AUTO_INCREMENT,\n  `nome` varchar(255) NOT NULL,\n  `preco` double NOT NULL,\n  `categoria` varchar(255) NOT NULL,\n  `imagem` mediumblob,\n  `pedido_id` bigint DEFAULT NULL,\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=36 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'



'produto_regiao', 'CREATE TABLE `produto_regiao` (\n  `id` bigint NOT NULL AUTO_INCREMENT,\n  `produto_id` bigint DEFAULT NULL,\n  `regiao` bigint DEFAULT NULL,\n  PRIMARY KEY (`id`),\n  UNIQUE KEY `uk_produto_regiao` (`id`),\n  KEY `fk_produto_regiao_produto` (`produto_id`),\n  CONSTRAINT `fk_produto_regiao_produto` FOREIGN KEY (`produto_id`) REFERENCES `produto` (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=91 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'



'produto_regiao_regioes', 'CREATE TABLE `produto_regiao_regioes` (\n  `produto_regiao_id` bigint NOT NULL,\n  `regiao` int NOT NULL,\n  PRIMARY KEY (`produto_regiao_id`,`regiao`),\n  CONSTRAINT `fk_produto_regiao_regioes_produto_regiao` FOREIGN KEY (`produto_regiao_id`) REFERENCES `produto_regiao` (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'



'regioes', 'CREATE TABLE `regioes` (\n  `id` int NOT NULL AUTO_INCREMENT,\n  `nome` varchar(255) NOT NULL,\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'

'users', 'CREATE TABLE `users` (\n  `id` int NOT NULL AUTO_INCREMENT,\n  `username` varchar(50) NOT NULL,\n  `password` varchar(255) NOT NULL,\n  `profile` enum(\'administrador\',\'vendedor\') NOT NULL,\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci'


